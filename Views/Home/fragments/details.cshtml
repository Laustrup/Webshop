@using Microsoft.AspNetCore.Identity

@inject SignInManager<IdentityUser> SignInManager

<section class="details">
        <dl class="row">
            <dt class="col-6 col-md-2">@Html.DisplayFor(Model => Model.Title)</dt>
            <dt class="d-none d-md-block col-md-6">@Html.DisplayFor(Model => Model.Description)</dt>
            <dt class="col-2 col-md-1"><p>Created </p>@Html.DisplayFor(Model => Model.Established)</dt>
        </dl>

        <div><a asp-action="edit">Edit details</a></div>
</section>